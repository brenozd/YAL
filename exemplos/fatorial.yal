def precision 0;
def round 0;

_init 
{
    outl 'Fatorial';
    let variavelA = 0;
    let resultado = 1;

    out 'vA: '; in variavelA;
    let x = variavelA;
    
    while(variavelA > 0){
        resultado *=  variavelA;
        variavelA -=  1;
    }

    out 'Fatorial de ';
    out x;
    out ' e igual a: ';
    outl resultado;
}