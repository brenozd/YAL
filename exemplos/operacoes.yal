def precision 2;
def round 0;

_init begin
    oult 'Operations';
    let variavelA;
    let variavelB;

    out 'A: '; in variavelA;
    out 'B: '; in variavelB;
    
    out '+ '; outl variavelA + variavelB;
    out '- '; outl variavelA - variavelB;
    out '* '; outl variavelA * variavelB;
    out '/ '; outl variavelA / variavelB;
    out '% '; outl variavelA % variavelB;
end